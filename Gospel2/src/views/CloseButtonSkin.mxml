<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark" alpha.disabled="0.5" xmlns:fc="http://ns.adobe.com/flashcatalyst/2009">
	<fx:Metadata>
        [HostComponent("spark.components.Button")]
   </fx:Metadata>
	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>
	<s:Ellipse alpha.up="0" x="-2" y="-2" width="11" height="11">
		<s:fill>
			<s:SolidColor color="0xdd0000" color.down="0x555555"/>
		</s:fill>
	</s:Ellipse>
	
	<s:Line id="line1" x="1" xTo="5" y="1" yTo="5">
		<s:stroke>
			<s:SolidColorStroke caps="round" color="#FFFFFF" joints="miter" miterLimit="4" weight="2"/>
		</s:stroke>
	</s:Line>
	<s:Line id="line0" x="1" xTo="5" y="1" yFrom="5">
		<s:stroke>
			<s:SolidColorStroke caps="round" color="#FFFFFF" joints="miter" miterLimit="4" weight="2"/>
		</s:stroke>
	</s:Line>
	<s:filters>
		<s:GlowFilter color="#FFFFFF" quality="3" blurX="2" blurY="2" strength="2" alpha.up="0" alpha.down="0"/>
	</s:filters>
</s:Skin>
